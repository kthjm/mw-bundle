{
  "name": "mw-bundle",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "pre": "chin && yarn wp:dll",
    "mb:dev": "microbundle watch src --o dist/microbundle/dev --target browser",
    "mb:dev:watch": "yarn watch dist/microbundle/dev",
    "mb:pro": "microbundle build src --o dist/microbundle/pro --target browser",
    "mb:pro:watch": "yarn watch dist/microbundle/pro",
    "par:dev": "cross-env BABEL_ENV=PARCEL parcel serve src/parcel/index.html --out-dir dist/parcel/dev --open",
    "par:pro": "cross-env BABEL_ENV=PARCEL parcel build src/parcel/index.html --out-dir dist/parcel/pro --no-cache",
    "poi:dev": "cross-env BABEL_ENV=POI poi develop src/poi",
    "poi:pro": "cross-env BABEL_ENV=POI poi build src --out-dir dist/poi/pro",
    "roll": "cross-env BABEL_ENV=ROLLUP rollup -c",
    "roll:watch": "yarn watch dist/rollup",
    "wp:dll": "cross-env BABEL_ENV=WEBPACK webpack --config webpack.config.dll.babel.js",
    "wp:dev": "cross-env BABEL_ENV=WEBPACK webpack-dev-server --config webpack.config.dev.babel.js",
    "wp:pro": "cross-env BABEL_ENV=WEBPACK webpack",
    "watch": "live-server",
    "dev": "run-p mb:dev par:dev poi:dev wp:dev",
    "pro": "run-s mb:pro par:pro poi:pro roll wp:pro"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chin": "^2.1.2",
    "cross-env": "^5.1.4",
    "live-server": "^1.2.0",
    "microbundle": "^0.4.4",
    "npm-run-all": "^4.1.2",
    "parcel-bundler": "^1.7.1",
    "poi": "^10.1.3",
    "react-hot-loader": "^4.1.1",
    "rollup": "^0.58.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-prettier": "^0.4.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-uglify": "^3.0.0",
    "uglify-es": "^3.3.9",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "atra": "^1.2.2",
    "orph": "^3.0.6",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  }
}
